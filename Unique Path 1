class Solution:
    def uniquePaths(self, m: int, n: int) -> int:
        memo = {}
        def helper(i, j):
            if i == 1 and j == 1:
                return 1
            if i < 1 or j < 1:
                return 0
            if (i, j) in memo:
                return memo[(i, j)]
            memo[(i, j)] = helper(i - 1, j) + helper(i, j - 1)
            return memo[(i, j)]   
        return helper(m, n)
